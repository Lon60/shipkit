type Mutation {
    register(input: CreateAccountInput!): AuthPayload!
    login(email: String!, password: String!): AuthPayload!
}

input CreateAccountInput {
    email: String!
    password: String!
}

type AuthPayload {
    token: String!
}

type Status {
    status: String!
    adminInitialized: Boolean!
}

type Query {
    status: Status!
    _empty: String
}
